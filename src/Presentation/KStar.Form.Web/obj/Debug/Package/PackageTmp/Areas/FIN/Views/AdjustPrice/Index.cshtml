
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>低价调价 表单</title>
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta http-equiv="Cache-Control" content="no-siteapp" />

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/lodash/lodash.min.js"></script>
    <script src="~/Scripts/axios/0.19.0/axios.min.js"></script>
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/vue/2.6.10/vue.js"></script>
    <script src="~/Scripts/element/2.8.2/index.js"></script>

    <link href="~/Scripts/element/2.8.2/theme-chalk/index.css" rel="stylesheet" />
    <link href="~/Content/css/reset.css" rel="stylesheet" />
    <link href="~/Content/iconfont/iconfont.css" rel="stylesheet" />
    <link href="~/Content/css/form/form.css" rel="stylesheet" />
    <link href="~/Content/css/components/headerSubtitle.css" rel="stylesheet" />
</head>
<body>
    <div id="app">
        <el-container>
            <!-- 头部header悬浮按钮工具栏 -->
            <headertoolbar :formtype="vmFormType" :formbtnmodel="vmKStarFormButtons" :formbackactivity="vmBackActivitys"></headertoolbar>
            <main class="wrap">
                <div class="form">
                    <!-- 标题头 -->
                    <headertitle :formheadmodel='vmFormInstance' :formprocessroute="vmProcessRoute"></headertitle>

                    <div class="center">
                        <!-- 当前处理人 -->
                        <!-- <moduleprocessingpeople></moduleprocessingpeople>-->
                        <!-- 基本信息 -->
                        <modulebasicinformation :formtype="vmFormType" :formbasicinformationmodel='vmFormInstance' ref="refComBasicInfo"></modulebasicinformation>

                        <!-- 流程说明 -->
                        @*<moduleprocessthat></moduleprocessthat>*@

                        <!-- 相关流程 -->
                        @*<modulerelatedprocesses></modulerelatedprocesses>*@

                        <!-- 表单信息 -->
                        <headersubtitle header-title="表单信息">
                            <i slot="subIcon" class="icon-sub icon iconfont iconbiaodanxinxi"></i>

                            <el-form :model="vmFormContent" :rules="formContentRules" ref="refFormContent" label-width="130px" class="demo-ruleForm" inline-message size="mini">
                                <el-row>
                                    <el-col :xs="24" :sm="24" :md="24" :lg="24">
                                        <div class="table-imitation-el-box">
                                            <table border="1" class="table-imitation-el" cellspacing="0" cellpadding="0" border="0">
                                                <thead>
                                                    <tr>
                                                        <th>
                                                            楼栋
                                                        </th>
                                                        <th>
                                                            单元
                                                        </th>
                                                        <th>
                                                            套数
                                                        </th>
                                                        <th>
                                                            总建筑面积（㎡）
                                                        </th>
                                                        <th>
                                                            首次核定单价（元/㎡）
                                                        </th>
                                                        <th>首次核定销售总额（元)</th>
                                                        <th>
                                                            上次核定单价（元/㎡）
                                                        </th>
                                                        <th>
                                                            上次核定销售总额（元）
                                                        </th>
                                                        <th>上次装修单价（元）</th>
                                                        <th>上次核定销售总额含装修款（元）</th>
                                                        <th>
                                                            本次核定建面均价（元/㎡）
                                                        </th>
                                                        <th>本次核定销售总额（元)</th>
                                                        <th>本次核定装修单价（元）</th>
                                                        <th>本次核定销售总额含装修款（元）</th>
                                                        <th>
                                                            调价幅度（基于首次）%
                                                        </th>
                                                        <th>
                                                            调价幅度（基于上次）%
                                                        </th>
                                                    </tr>
                                                </thead>


                                                <tbody>
                                                    <tr>
                                                        <td>
                                                            <el-input v-model="vmFormContent.a1" size="mini"></el-input>
                                                        </td>
                                                        <td><el-input v-model="vmFormContent.a2" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a3" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a4" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a5" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a6" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a7" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a8" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a9" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a10" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a11" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a12" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a13" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a14" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a15" size="mini"></el-input></td>
                                                        <td><el-input v-model="vmFormContent.a16" size="mini"></el-input></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="pull-right">
                                                            说明
                                                        </td>
                                                        <td colspan="15"><el-input v-model="vmFormContent.a17" type="textarea" size="mini"></el-input></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="pull-right">
                                                            综合折扣率
                                                        </td>
                                                        <td colspan="15"><el-input v-model="vmFormContent.a18" type="textarea" size="mini"></el-input></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="pull-right">
                                                            拓展版利润率
                                                        </td>
                                                        <td><el-input v-model="vmFormContent.a19" size="mini"></el-input></td>
                                                        <td class="pull-right">方案版利润率</td>
                                                        <td><el-input v-model="vmFormContent.a20" size="mini"></el-input></td>
                                                        <td class="pull-right">当前利润率</td>
                                                        <td><el-input v-model="vmFormContent.a21" size="mini"></el-input></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="pull-right">
                                                            核价说明
                                                        </td>
                                                        <td colspan="15">
                                                            <div>
                                                                1、调价核价审批需提供附件：《销售进度表》、《销售价格审批表》、《返点明细表》及《价格报告》（包括但不限于竞品市场、项目分析、调价策略等）
                                                                2、利润率测算由营销部提供相关测算数据为附件；各阶段利润率由一线公司财务审批中填写，抄送财务中心
                                                                3、商铺核价需提供商铺位置平面图
                                                                4、核价说明中需要注明此次核价上报依据哪次会议
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </el-col>
                                </el-row>
                                <subtitle subtitle="测试参数部分"></subtitle>
                                <el-row>
                                    <el-col :xs="24" :sm="24" :md="12" :lg="8">
                                        <el-form-item label="公司名称" prop="CompanyName">
                                            <el-select v-model="vmFormContent.CompanyName" placeholder="请选择公司" size="mini">
                                                <el-option label="中山" value="中山"></el-option>
                                                <el-option label="肇庆" value="肇庆"></el-option>
                                                <el-option label="上海" value="上海"></el-option>
                                                <el-option label="成都" value="成都"></el-option>
                                                <el-option label="河源" value="河源"></el-option>
                                                <el-option label="海南" value="海南"></el-option>
                                                <el-option label="防城港" value="防城港"></el-option>
                                                <el-option label="桂林" value="桂林"></el-option>
                                                <el-option label="柳州" value="柳州"></el-option>
                                                <el-option label="广清" value="广清"></el-option>
                                                <el-option label="东莞" value="东莞"></el-option>
                                                <el-option label="深圳" value="深圳"></el-option>
                                                <el-option label="南宁" value="南宁"></el-option>
                                                <el-option label="惠州" value="惠州"></el-option>
                                                <el-option label="汕头" value="汕头"></el-option>
                                                <el-option label="佛山" value="佛山"></el-option>
                                                <el-option label="珠海" value="珠海"></el-option>
                                                <el-option label="神湾" value="神湾"></el-option>
                                                <el-option label="阳江" value="阳江"></el-option>
                                            </el-select>
                                        </el-form-item>
                                    </el-col>
                                    <el-col :xs="24" :sm="24" :md="12" :lg="8">
                                        <el-form-item label="项目名称" prop="ProjectName">
                                            <el-select v-model="vmFormContent.ProjectName" placeholder="请选择项目" size="mini">
                                                <el-option label="潮州" value="潮州"></el-option>
                                                <el-option label="清远" value="清远"></el-option>
                                            </el-select>
                                        </el-form-item>
                                    </el-col>
                                    <el-col :xs="24" :sm="24" :md="12" :lg="8">
                                        <el-form-item label="基于首次定价调价幅度" prop="PriceRange">
                                            <el-input v-model="vmFormContent.PriceRange" size="mini"></el-input>
                                        </el-form-item>
                                    </el-col>
                                </el-row>
                        </headersubtitle>

                        <!-- 附件 -->
                        <moduletheattachment></moduletheattachment>

                        <!-- 步骤信息 -->
                        @*<modulestepinformation :formtype="vmFormType"></modulestepinformation>*@
                        <!-- 流程处理 -->
                        <moduleprocesstodealwith v-if="vmFormType=='Approval'||vmFormType=='ReApproval'||vmFormType=='CommunicateFeedback'" :formprocesstodealwithmodel="vmFormSetting" :formoperationmodel="vmOperation" ref="refComProcessToDeal"></moduleprocesstodealwith>

                        <!-- 审批记录 -->
                        <moduleApprovalRecords v-if="vmFormType!='Application'" :formapprovalmodel="vmFormApprovalHistorys"></moduleApprovalRecords>
                    </div>
                </div>
            </main>
        </el-container>

    </div>


    <script src="~/Scripts/components/headerToolbar.js"></script>
    <script src="~/Scripts/components/headertitle.js"></script>
    <script src="~/Scripts/components/headerSubtitle.js"></script>
    <script src="~/Scripts/components/moduleProcessingPeople.js"></script>
    <script src="~/Scripts/components/moduleBasicInformation.js"></script>
    <script src="~/Scripts/components/moduleProcessThat.js"></script>
    <script src="~/Scripts/components/moduleRelatedProcesses.js"></script>
    <script src="~/Scripts/components/moduleTheAttachment.js"></script>
    <script src="~/Scripts/components/moduleStepInformation.js"></script>
    <script src="~/Scripts/components/moduleProcessToDealWith.js"></script>
    <script src="~/Scripts/components/moduleApprovalRecords.js"></script>
    <script src="~/Scripts/components/subtitlet.js"></script>
    <script src="~/Scripts/components/userRemotePick.js"></script>
    <script src="~/Scripts/IE-promise.js"></script>
    <script src="~/Areas/FIN/Models/AdjustPrice.js"></script>
    <script src="~/Scripts/formmain.js"></script>

    <script type="text/javascript">


        //初始化数据后绑定之前
        KStarForm.initFormDataAfter = function () {

        }
        //扩展
        KStarForm.formModelExtend = {
            data: function () {
                return {

                    formContentRules: {
                        SingleIndex: [
                            { required: true, message: '请输入单项指标', trigger: 'blur' }
                        ],
                        ContractCopies: [
                            { required: true, message: '请输入合同份数', trigger: 'blur' },
                        ],
                        SealType: [
                            { required: true, message: '请输入印章类型', trigger: 'blur' },
                        ],
                        IsOverweight: [
                            { required: true, message: '请输入对比情况是否超标', trigger: 'blur' },
                        ],
                        IsUseCompanyContractCopy: [
                            { required: true, message: '请输入是否使用公司合同文本', trigger: 'blur' },
                        ]
                    }
                }
            },
            //方法
            methods: {
                hello: function () {
                    alert("aa");
                },
                alert2: function () {
                    alertInfo.call(this, "好样的！！");
                }
            },
            //计算属性
            computed: {

            }

        }
        KStarForm.init();
    </script>
</body>




</html>